{
  "name": "suite-device-app",
  "version": "1.0.3",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Dualbase",
  "homepage": "https://www.dhionecastilho.com.br",
  "repository": "https://github.com/DhioneCastilhoBarbosa/suite-device-app",
  "publish": {
    "provide": "github",
    "releaseType": "release"
  },
  "config": {
    "forge": "forge.config.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "package": "electron-forge package",
    "start-forge": "electron-forge start"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@phosphor-icons/react": "^2.0.15",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@serialport/parser-byte-length": "^12.0.0",
    "@serialport/parser-readline": "^12.0.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.1",
    "file-saver": "^2.0.5",
    "modbus-serial": "^8.0.16",
    "serialport": "^10.5.0",
    "tailwind-merge": "^2.2.2",
    "update-electron-app": "^3.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-wix": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "@electron-forge/plugin-vite": "^7.3.0",
    "@electron-forge/publisher-github": "^7.3.0",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-packager": "^17.1.2",
    "electron-vite": "^1.0.29",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
